#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_723b897db5f4e537fb87bdbe1df9f8a6/node_modules/ts-jest/node_modules:/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_723b897db5f4e537fb87bdbe1df9f8a6/node_modules:/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_723b897db5f4e537fb87bdbe1df9f8a6/node_modules/ts-jest/node_modules:/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/ts-jest@29.4.4_@babel+core@7.28.4_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_723b897db5f4e537fb87bdbe1df9f8a6/node_modules:/Users/monordi/Documents/Operation/Informatique/EduAI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
